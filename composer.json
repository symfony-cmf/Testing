{
    "name": "symfony-cmf/testing",
    "license": "MIT",
    "description": "Component providing tools for writing tests with Symfony.",
    "authors": [
        {
            "name": "Symfony CMF Community",
            "homepage": "https://github.com/symfony-cmf/Routing/contributors"
        }
    ],
    "require": {
        "php": "^7.1",
        "doctrine/common": "^2.7",
        "doctrine/data-fixtures": "^1.2",
        "doctrine/doctrine-bundle": "^1.8 || ^2.0",
        "doctrine/phpcr-bundle": "^1.3 || ^2.0.0",
        "doctrine/phpcr-odm": "^1.4",
        "friendsofsymfony/jsrouting-bundle": "^1.1 || ^2.1",
        "jackalope/jackalope": "^1.3",
        "jackalope/jackalope-doctrine-dbal": "^1.3",
        "symfony/browser-kit": "^3.4 || ^4.0",
        "symfony/console": "^3.4 || ^4.0",
        "symfony/css-selector": "^3.4 || ^4.0",
        "symfony/debug": "^3.4 || ^4.0",
        "symfony/dependency-injection": "^3.4 || ^4.0",
        "symfony/doctrine-bridge": "^3.4 || ^4.0",
        "symfony/framework-bundle": "^3.4 || ^4.0",
        "symfony/http-foundation": "^3.4 || ^4.0",
        "symfony/http-kernel": "^3.4 || ^4.0",
        "symfony/monolog-bundle": "~3.1",
        "symfony/process": "^3.4 || ^4.0",
        "symfony/security-bundle": "^3.4 || ^4.0",
        "symfony/twig-bundle": "^3.4 || ^4.0",
        "symfony/phpunit-bridge": "4.2.2"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.4 || ^4.0"
    },
    "autoload": {
        "psr-4": {
            "Symfony\\Cmf\\Component\\Testing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symfony\\Cmf\\Component\\Testing\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    }
}
